{
  "name": "discord-sb.js",
  "version": "1.3.5",
  "description": "An unofficial discord.js fork for creating selfbots",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "all": "bun run build && bun publish",
    "test": "bun run lint:all && bun run docs:test && bun run test:typescript",
    "test:unit": "bun test",
    "fix:all": "bun run lint:fix && bun run lint:typings:fix && bun run format",
    "test:typescript": "tsc --noEmit && tsd",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "lint:typings:fix": "tslint typings/index.d.ts --fix",
    "format": "prettier --write src/**/*.js typings/**/*.ts",
    "lint:all": "bun run lint && bun run lint:typings",
    "docs": "docgen --source src --custom docs/index.yml --output docs/main.json",
    "docs:test": "docgen --source src --custom docs/index.yml",
    "build": "bun run lint:fix && bun run lint:typings:fix && bun run format && bun run docs"
  },
  "files": [
    "src",
    "typings"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqlu/discord-sb.js.git"
  },
  "keywords": [
    "discord.js",
    "discord.js v13",
    "selfbot",
    "selfbot v13",
    "djs",
    "api",
    "bot",
    "node",
    "discord",
    "client",
    "discordapp"
  ],
  "author": "Snayz",
  "license": "GNU General Public License v3.0",
  "bugs": {
    "url": "https://github.com/sqlu/discord-sb.js/issues"
  },
  "homepage": "https://github.com/sqlu/discord-sb.js#readme",
  "dependencies": {
    "@discordjs/builders": "^1.13.1",
    "@discordjs/collection": "^2.1.1",
    "@sapphire/async-queue": "^1.5.5",
    "@sapphire/shapeshift": "^4.0.0",
    "debug": "^4.4.3",
    "discord-api-types": "^0.38.40",
    "otplib": "^12.0.1",
    "prism-media": "^1.3.5",
    "qrcode": "^1.5.4",
    "werift-rtp": "^0.8.8"
  },
  "engines": {
    "bun": ">=1.3.5"
  },
  "devDependencies": {
    "@discordjs/docgen": "^0.11.1",
    "@types/bun": "^1.3.9",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.19.11",
    "dtslint": "^4.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.5",
    "patch-package": "^8.0.1",
    "prettier": "^2.8.8",
    "tsd": "^0.32.0",
    "tslint": "^6.1.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.5"
}
